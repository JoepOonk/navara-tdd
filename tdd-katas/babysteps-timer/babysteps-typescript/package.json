{
  "name": "babystep-kata",
  "version": "1.0.0",
  "description": "BabyStep kata in TypeScript",
  "scripts": {
    "precompile": "rimraf babystep/**/*.js spec/**/*.js",
    "compile": "tsc",
    "pretest": "rimraf babystep/**/*.js spec/**/*.js",
    "test": "mocha -r ts-node/register -r jsdom-global/register --file spec/global-setup.ts spec/*_spec.ts",
    "serve": "npx http-server -a 0.0.0.0 -p 8088 -o babystep.html",
    "watch": "npx mocha -r ts-node/register -r jsdom-global/register --file spec/global-setup.ts spec/*_spec.ts --watch --extension ts"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/jsdom-global": "3.0.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "~7.0.18",
    "chai": "^4.1.2",
    "jsdom": "^16.7.0",
    "jsdom-global": "3.0.2",
    "mocha": "^10.2.0",
    "ts-node": "7.0.0",
    "typescript": "3.9.5",
    "http-server": "^14.1.1",
    "rimraf": "~2.5.2"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "text"
    ]
  },
  "dependencies": {
  }
}
